{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"properties": {
		"path": {
			"type": "string",
			"description": "路由路径"
		},
		"icon": {
			"type": "string",
			"description": "路由图标"
		},
		"title": {
			"type": "string",
			"description": "路由名"
		},
		"redirect": {
			"type": "string",
			"description": "跳转子路由信息"
		}
	},
	"required": ["path", "icon", "title", "redirect"]
}
