{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"sourceMap": false,
		"moduleResolution": "Node",
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"removeComments": false,
		"importHelpers": true,
		"strict": true,
		"baseUrl": "./",
		"paths": {
			"@/*": ["src/*"]
		}
	}
}
